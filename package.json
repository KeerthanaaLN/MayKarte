{
  "name": "MayKarte",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-bookings": "cds watch --open maykarte.bookings.bookings/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy MayKarte --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-maykarteui": "cds watch --open maykarteui/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-maykarte222": "cds watch --open maykarte222/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "cds": {},
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/bookings",
    "app/bookings"
  ]
}
